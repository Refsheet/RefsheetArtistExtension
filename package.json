{
  "name": "refsheet-adobe-extension",
  "version": "1.0.104",
  "description": "Refsheet.net Adobe Artist Extension",
  "main": "src/ext.js",
  "directories": {
    "src/lib": "src/lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "version": "node bin/version set",
    "release": "node bin/release",
    "install": "node bin/install",
    "bundle": "cd src && npm install --production --no-bin-links --no-optional",
    "build": "npm run bundle && node bin/build",
    "build:release": "npm run version && npm run build && npm run release",
    "build:install": "npm run build && npm run install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Refsheet/RefsheetArtistExtension.git"
  },
  "author": "William Eisert <weisert@eisertdev.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Refsheet/RefsheetArtistExtension/issues"
  },
  "homepage": "https://github.com/Refsheet/RefsheetArtistExtension#readme"
}